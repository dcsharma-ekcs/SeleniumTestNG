<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="tests" >

    <test name="addNewUserChrome">
        <parameter name="browserName" value="chrome"></parameter>
        <classes>
            <class name="io.vtcustomer.AddNewUser">
                <methods>
                    <include name="vtAddUser"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="addNewUserFirefox">
        <parameter name="browserName" value="firefox"></parameter>
        <classes>
            <class name="io.vtcustomer.AddNewUserMultipleBroser">
                <methods>
                    <include name="vtAddUser"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>